{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "orry"
		},
		"CosmosDbBookings_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'CosmosDbBookings'"
		},
		"ADLS_Customer_Data_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://synapsedh.dfs.core.windows.net/"
		},
		"AzureSynapseAnalyticsDWH_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "synapsis.sql.azuresynapse.net"
		},
		"AzureSynapseAnalyticsDWH_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "dedicatedPool"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "orry_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"CosmosDbBookings_connectionString": {
						"value": "[parameters('CosmosDbBookings_connectionString')]"
					},
					"ADLS_Customer_Data_properties_typeProperties_url": {
						"value": "[parameters('ADLS_Customer_Data_properties_typeProperties_url')]"
					},
					"AzureSynapseAnalyticsDWH_properties_typeProperties_server": {
						"value": "[parameters('AzureSynapseAnalyticsDWH_properties_typeProperties_server')]"
					},
					"AzureSynapseAnalyticsDWH_properties_typeProperties_database": {
						"value": "[parameters('AzureSynapseAnalyticsDWH_properties_typeProperties_database')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}